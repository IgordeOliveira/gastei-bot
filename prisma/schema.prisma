// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model User {
  id          Int    @id @default(autoincrement())
  telegram_id String @unique
  document_id String
  sheet_id    Int
}

model Session {
  id    Int    @id @default(autoincrement())
  key   String @unique
  value String
}
